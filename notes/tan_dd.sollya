pretty = proc(u) {
  return ~(floor(u*1000)/1000);
};

d = [0, pi/128];
f_tan = tan(x)/x;
Q = fpminimax(f_tan, [|0, 2, 4, 6, 8, 10, 12|], [|1, 107...|], d);

err_p = -log2(dirtyinfnorm(Q-f_tan, d));
display = decimal;
print (Q);
display = decimal;
print ("absolute error:", pretty(err_p));

for i from 0 to degree(Q) by 2 do print(coeff(Q, i));