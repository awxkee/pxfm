pretty = proc(u) {
  return ~(floor(u*1000)/1000);
};

d = [-2^-4, 2^-4];
f_expm1f = (expm1(x) - x)/x^2;
Q = fpminimax(f_expm1f, 6, [|D...|], d);

err_p = -log2(dirtyinfnorm(Q-f_expm1f, d));
display = decimal;
print (Q);
display = decimal;
print ("absolute error:",pretty(err_p));

for i from 0 to degree(Q) by 1 do printdouble(coeff(Q, i));